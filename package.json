{
  "name": "@bundles/bundles-ejs",
  "version": "0.0.1",
  "description": "A bundler plugin for Bundles which compiles templates with EJS.",
  "keywords": [
    "brikcss",
    "bundles",
    "bundler",
    "ejs"
  ],
  "license": "MIT",
  "author": "Brikcss <thezimmee@gmail.com> (https://github.com/brikcss/)",
  "contributors": [
    "The Zimmee <thezimmee@gmail.com>"
  ],
  "homepage": "https://github.com/brikcss/bundles-ejs",
  "repository": {
    "type": "git",
    "url": "https://github.com/brikcss/bundles-ejs.git"
  },
  "bugs": "https://github.com/brikcss/bundles-ejs/issues",
  "files": [
    "lib"
  ],
  "main": "lib/bundles-ejs.js",
  "module": "module/bundles-ejs.js",
  "publishConfig": {
    "tag": "dev",
    "access": "public"
  },
  "scripts": {
    "start": "npm run watch",
    "pretest": "npm run build",
    "test": "standard *.js && jest --coverage && cat coverage/lcov.info | coveralls && cat coverage/lcov.info | codacy-coverage",
    "unit": "jest --config=.jestrc.js",
    "prebuild": "npx rimraf {lib,module}",
    "build": "rollup --config=.rolluprc.js",
    "watch": "npx concurrently \"npm run unit -- --watchAll\" \"npm run build -- --watch\""
  },
  "devDependencies": {
    "@babel/preset-env": "^7.3.1",
    "@brikcss/git-hooks": "^2.1.0",
    "@node-minify/cli": "^4.1.0",
    "@node-minify/uglify-es": "^4.0.5",
    "codacy-coverage": "^3.4.0",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.2",
    "jest": "^24.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-terser": "^4.0.4",
    "semantic-release": "^15.13.3"
  },
  "dependencies": {
    "@bundles/core": "^0.3.1",
    "ejs": "2.6.1"
  }
}
